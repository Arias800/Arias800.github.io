<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.wnbc"
       name="WNBC Programs"
       version="4.0.1"
       provider-name="t1m/ Arias800">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
    <import addon="script.module.t1mlib" version="4.0.0"/>
    <import addon="script.module.requests" version="2.22.0"/>
    <import addon="inputstream.adaptive" version="2.6.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">WNBC Programs</summary>
    <description lang="en_GB">WNBC Programs. The Tonight Show, NBC News and NBC Classic TV.</description>
    <disclaimer lang="en_GB">Feel free to use this script. For information visit the wiki.</disclaimer>
    <platform>all</platform>
    <language>en</language>
    <forum>https://forum.kodi.tv/showthread.php?tid=234448</forum>
    <website>nbc.com</website>
    <source>https://github.com/learningit/repo-plugins/</source>
    <license>GPL-2.0-or-later</license>
    <news>Version 4.0.1 Add working subtitles
    </news>
    <assets>
        <icon>resources/icon.png</icon>
        <fanart>resources/fanart.jpg</fanart>
    </assets>
  </extension>
</addon>

<addon id="repository.arias800" name="Arias800's Repository" version="0.1" provider-name="Arias800">
    <extension point="xbmc.addon.repository" name="Arias800's Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/Arias800/repository.arias800/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Arias800/repository.arias800/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Arias800/repository.arias800/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Arias800's Repository</summary>
        <description>DESCRIPTION OF YOUR REPO HERE</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

<addon id="script.module.dnspython"
       name="DNS Python"
	   version="1.12.1"
	   provider-name="dnspython">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.module" library="lib" />
  <extension point="xbmc.addon.metadata">
	<summary>A Pure Python DNS module</summary>
	<description>A Pure Python DNS module</description>
	<disclaimer lang="en"></disclaimer>
	<language>en</language>
    <platform>all</platform>
	<license>BSD-style license</license>
	<forum></forum>
	<website>http://www.dnspython.org/</website>
	<email></email>
	<source>https://github.com/Arias800/script.module.dnspython/</source>
  </extension>
</addon>
</addons>
